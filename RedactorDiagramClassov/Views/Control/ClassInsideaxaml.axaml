<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:RedactorDiagramClassov.Views.Control"
		xmlns:models="clr-namespace:RedactorDiagramClassov.Models">
		
	<Design.PreviewWith>
		<controls:ClassInsideaxaml />	
  </Design.PreviewWith>

  <Style Selector="controls|ClassInsideaxaml">
    <!-- Set Defaults -->
    <Setter Property="Template">
		<ControlTemplate>
			<Line StartPoint="{TemplateBinding CustomX}" EndPoint="{TemplateBinding CustomY}" Stroke="Black" StrokeThickness="1"></Line>
		</ControlTemplate>
    </Setter>
  </Style>
</Styles>
